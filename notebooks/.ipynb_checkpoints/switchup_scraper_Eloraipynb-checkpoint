   "source": [
    "comments['overallScore'] = pd.to_numeric(comments['overallScore'], errors='coerce')\n",
    "\n",
    "comments.dropna(subset=['overallScore'], inplace=True)  # Drop rows with missing values\n",
    "\n",
    "average_scores_per_school = comments.groupby('school')['overallScore'].mean().reset_index()\n",
    "\n",
    "average_scores_per_school.columns = ['school_name', 'average_overall_score']\n",
    "\n",
    "display(average_scores_per_school)"
   ]
