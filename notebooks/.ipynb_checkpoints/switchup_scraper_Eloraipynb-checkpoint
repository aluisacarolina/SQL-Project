    "top_programs_with_scores = comments.groupby(['school', 'program']).agg(\n",
    "    number_of_reviews=('program', 'size'),\n",
    "    average_overall_score=('overallScore', 'mean')\n",
    ").reset_index()\n",
    "\n",
    "top_programs_with_scores = top_programs_with_scores.sort_values(by='number_of_reviews', ascending=False)\n",
    "\n",
    "top_ten_programs = top_programs_with_scores.head(10)\n",
    "\n",
    "print(\"Top ten programs reviewed with average overall scores (sorted by number of reviews):\")\n",
    "display(top_ten_programs)"
   ]
  },
